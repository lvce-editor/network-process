{
  "name": "@lvce-editor/network-process",
  "version": "0.0.0-dev",
  "description": "Network Process",
  "main": "src/networkProcessMain.js",
  "type": "module",
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "type-check": "tsc",
    "postinstall": "lerna bootstrap --ci"
  },
  "keywords": [
    "Lvce Editor",
    "Network",
    "Utility"
  ],
  "author": "Lvce Editor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvce-editor/network-process.git"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@lvce-editor/assert": "^1.3.0",
    "@lvce-editor/ipc": "^13.8.0",
    "@lvce-editor/json-rpc": "^5.4.0",
    "@lvce-editor/verror": "^1.6.0",
    "execa": "^9.5.2",
    "got": "^14.4.7",
    "symlink-dir": "^6.0.5",
    "tar-fs": "^3.0.8",
    "tmp-promise": "^3.0.3",
    "trash": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@types/tar-fs": "^2.0.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.1",
    "@lerna/legacy-package-management": "^8.2.2",
    "@lvce-editor/eslint-config": "^1.38.0",
    "eslint": "^9.24.0",
    "lerna": "^8.2.2",
    "typescript": "^5.8.3"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "@vscode/windows-process-tree": "<rootDir>/mocks/windows-process-tree.js",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "optionalDependencies": {
    "open": "^10.1.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
